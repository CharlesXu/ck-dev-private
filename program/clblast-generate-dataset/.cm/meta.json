{
  "backup_data_uid": "6b29779ab49f449f",
  "data_name": "clblast-generate-dataset",
  "no_compile": "yes",
  "no_target_file": "yes",
  "print_files_after_run": [
    "tmp-stdout.tmp",
    "tmp-stderr.tmp"
  ],
  "process_in_tmp": "yes",
  "program": "yes",
  "run_cmds": {
    "Power-Of-Two": {
      "ignore_return_code": "no",
      "run_time": {
        "output_invariant_of_repeat": "yes",
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../generate_dataset.py --kernel $<<CK_KERNEL_LIST>>$ --platform=$<<CK_TARGET_ARCHITECTURE>>$ --output_dir=$<<CK_OUTPUT_DIR>>$/Antonet --load_matrix_from_csv ../dataset/power_of_two.csv",
        "run_cmd_out1": "tmp-stdout.tmp",
        "run_cmd_out2": "tmp-stderr.tmp",
        "run_output_files": [
          "tmp-ck-timer.json",
          "tmp-ck-output.bin",
          "tmp-ck-output.json"
        ]
      }
    },
    "Grid-of-Two": {
      "ignore_return_code": "no",
      "run_time": {
        "output_invariant_of_repeat": "yes",
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../generate_dataset.py --kernel $<<CK_KERNEL_LIST>>$ --platform=$<<CK_TARGET_ARCHITECTURE>>$ --output_dir=$<<CK_OUTPUT_DIR>>$/Antonet --load_matrix_from_csv ../dataset/grid_of_two.csv",
        "run_cmd_out1": "tmp-stdout.tmp",
        "run_cmd_out2": "tmp-stderr.tmp",
        "run_output_files": [
          "tmp-ck-timer.json",
          "tmp-ck-output.bin",
          "tmp-ck-output.json"
        ]
      }
    },
    "anton-net": {
      "ignore_return_code": "no",
      "run_time": {
        "output_invariant_of_repeat": "yes",
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../generate_dataset.py --kernel $<<CK_KERNEL_LIST>>$ --platform=$<<CK_TARGET_ARCHITECTURE>>$ --output_dir=$<<CK_OUTPUT_DIR>>$/Antonet --load_matrix_from_csv ../dataset/AntonNet.csv",
        "run_cmd_out1": "tmp-stdout.tmp",
        "run_cmd_out2": "tmp-stderr.tmp",
        "run_output_files": [
          "tmp-ck-timer.json",
          "tmp-ck-output.bin",
          "tmp-ck-output.json"
        ]
      }
    }
  },
  "run_deps": {
    "lib-clblast": {
      "local": "yes",
      "name": "CLBlast MultiConf ",
      "skip_pruning_by_other_deps": "yes",
      "sort": 10,
      "tags": "lib,clblast,multiconf,tune"
    },
    "python": {
      "local": "yes",
      "name": "python",
      "sort": 8,
      "tags": "compiler,lang-python,python"
    }
  },
  "run_vars": {
      "CK_KERNEL_LIST": "xgemm xgemm_direct",
      "CK_TARGET_ARCHITECTURE": "Nvidia_P100",
      "CK_OUTPUT_DIR": "/tmp"
  },
  "tags": [
    "generate",
    "dataset",
    "program",
    "multiconf",
    "clblast"
  ]
}
